dev6 ansible_ssh_host='address6' zowner_DOMAIN='devel'
dev7 ansible_ssh_host='address7' zowner_DOMAIN='devel'
dev8 ansible_ssh_host='address8' zowner_DOMAIN='devel'
dev9 ansible_ssh_host='address9' zowner_DOMAIN='devel'
l1 ansible_ssh_host='address10' zowner_DOMAIN='l1'
l3 ansible_ssh_host='address11' zowner_DOMAIN='l3'
l4 ansible_ssh_host='address12' zowner_DOMAIN='l4'

[test]
